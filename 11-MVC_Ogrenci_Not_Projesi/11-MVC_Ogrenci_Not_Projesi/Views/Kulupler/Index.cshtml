@using _11_MVC_Ogrenci_Not_Projesi.Models.EntityFramework
@model List<TBLKULUPLER>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<table class="table table-bordered">
    <tr>
        <th>KULÜP ID</th>
        <th>KULÜP ADI</th>
        <th>İŞLEMLER</th>
    </tr>
    <tbody>
        @foreach (var i in Model)
        {
        <tr>
            <td><b>@i.KULUPID</b></td>
            <td>@i.KULUPAD</td>
            <td><a href="/Kulupler/Sil/@i.KULUPID" class="btn btn-danger">SİL</a>&nbsp;
            <a href="/Kulupler/Getir/@i.KULUPID" class="btn btn-success">GÜNCELLE</a></td>
        </tr>
        }
    </tbody>
</table>
<a href="/Kulupler/YeniKulup" class="btn btn-primary">Yeni Kulüp Kaydı</a>


